{{> header}}




<div id="playedReports" class="container report-list__wrap">

    <div id="playedReportsList" class="report-list played-report-list row mt-4">
        <div class="col-md-8">
            <div class="report__wrap p-3">
                <div class="report-header__wrap d-flex align-items-center">
                    <img class="mr-2" src="/img/logo.png" height="18px" width="18px" alt="">
                    <div class="report-time-stamp text-truncate" id="reportTimeStamp"><small>2019/11/25 12:12:12</small>
                    </div>
                </div>
                <div class="report-title mt-1" id="gameName">Hardware Identification - Refresher</div>
                <div class="report-hosted text-muted text-truncate"><small>Hosted by
                        <span id="creator">Zhi Xiang Lee</span></small></div>
            </div>
        </div>
    </div>




    <div class="row indicators__wrap mt-4">
        <div class="col-sm-12 col-md-8 col-lg-8">
            <div class="row no-gutters">
                <div class="col-4">
                    <div class="indicator__wrap d-flex flex-column justify-content-center align-items-center p-3 mr-1">
                        <div class="indicator-box" id="gameAccuracy">25%</div>
                        <div class="indicator-label text-center w-100 py-2 mt-2">Accuracy</div>
                    </div>
                </div>
                <div class="col-4">
                    <div
                        class="indicator__wrap d-flex flex-column justify-content-center align-items-center p-3 ml-1 mr-1">
                        <div class="indicator-box" id="totalQuestions">25</div>
                        <div class="indicator-label text-center w-100 py-2 mt-2">Questions</div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="indicator__wrap d-flex flex-column justify-content-center align-items-center p-3 ml-1">
                        <div class="indicator-box" id="totalPlayers">25</div>
                        <div class="indicator-label text-center w-100 py-2 mt-2">Players</div>
                    </div>
                </div>
            </div>
        </div>
    </div>










